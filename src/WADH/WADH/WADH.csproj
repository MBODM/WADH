<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <PropertyGroup>
    <Version>3.0.0</Version>
    <ApplicationIcon>WADH.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="WADH.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Web.WebView2" Version="1.0.1722.45" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\WADH.Core\WADH.Core.csproj" />
    <ProjectReference Include="..\WADH.WebView\WADH.WebView.csproj" />
  </ItemGroup>

</Project>
